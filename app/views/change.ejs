var vm = new Vue({
    el: '#vueApp',
    data: {
        changeRequestJSON: null
    },
    created: function created() {
        var _this  = this;
        $.getJSON('/getchangebyid?id=<%=req.query.id%>', function (data) {
            _this.changeRequestJSON = data; //Get Change request data and put into a variable
        });
    }

})

